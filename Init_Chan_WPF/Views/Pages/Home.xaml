<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Init_Chan_WPF.Views.Pages"
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml" x:Class="Init_Chan_WPF.Views.Pages.Home"
      mc:Ignorable="d"
      ui:Design.Background="{DynamicResource ApplicationBackgroundBrush}"
      ui:Design.Foreground="{DynamicResource TextFillColorPrimaryBrush}"
      Foreground="{DynamicResource TextFillColorPrimaryBrush}"
      ScrollViewer.CanContentScroll="False"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Home">

    <Grid ShowGridLines="True">
        <ui:VirtualizingItemsControl Width="NaN">
            <ui:VirtualizingItemsControl.Foreground>
                <ImageBrush/>
            </ui:VirtualizingItemsControl.Foreground>
        </ui:VirtualizingItemsControl>
    </Grid>
</Page>
